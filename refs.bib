@article{peterson,
  title = {The {{Private Interests}} of {{Public Officials}}: Financial {{Regulation}} in the {{US Congress}}},
  author = {Peterson, Jordan Carr and Grose, Christian R.},
  journaltitle = {Legislative Studies Quarterly},
  doi = {10.1111/lsq.12294},
  url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/lsq.12294?af=R&utm_source=researcher_app&utm_medium=referral&utm_campaign=RESR_MRKT_Researcher_inbound&sid=researcher},
  abstract = {{$<$}p{$>$}Legislators' private financial holdings affect policy decisions. Due to financial selfâ€interest, we theorize that legislators whose personal investment portfolios include equities from firms affected by proposed policies vote for legislation that benefits those firms. We also theorize that legislators with greater personal exposure to equity investments support policies that benefit equities markets generally. We create a novel data set of legislators' personal stock investments and examine major congressional actions since the 1990s on financial deregulation and market intervention. US House members who own stocks in firms who benefit from financial deregulation vote for deregulation. House members with greater exposure to financial and automotive stocks support the financial and auto bailouts, respectively. General exposure to equities markets is also associated with support for key legislation boosting markets. The normative implications are significant, as legislators' private interests influence decisions in the public sphere.{$<$}/p{$>$}},
  keywords = {Researcher App}
}

@article{peterson1998,
  title = {The {{Continuing Puzzle}} of {{Committee Outliers}}: A {{Methodological Reassessment}}},
  shorttitle = {The {{Continuing Puzzle}} of {{Committee Outliers}}},
  author = {Peterson, Geoffrey D. and Wrighton, J. Mark},
  date = {1998-03},
  journaltitle = {Congress \& the Presidency},
  shortjournal = {Congress \& the Presidency},
  volume = {25},
  number = {1},
  pages = {67--78},
  issn = {0734-3469, 1944-1053},
  doi = {10.1080/07343469809507809},
  url = {http://www.tandfonline.com/doi/abs/10.1080/07343469809507809},
  urldate = {2020-02-17},
  abstract = {Congressional parties play a pivotal role in the selection of committee members. The parties' committee composition decisions clearly affect the natures of the committees. Given the importance of party as an organizing force in the United States Congress, we expect that the committee selection process is a nonrandom one driven by the decisions of a party's leadership. Further, failure to account for party contingents within committees misspecifies the assignment process. In this article, we assert that the inclusion of party contingents should produce significantly different results from previous simulation efforts that failed to account for them. Finally, we expect committees with particularized interests will possess party contingents which are ideologically distinguishable from the House, while those that affect a wider range of interests will have contingents indistinguishable from their parent chamber. Employing a Monte Carlo simulation technique and Wilcoxon differenceof-medians tests using multiple interest group ratings, we find that properly specifying the process by including party contingents in the analysis generates a greater number of ideologically outlying committees than does previous research.},
  langid = {english},
  file = {/Users/nick/Documents/Research/Articles/Peterson_Wrighton_1998_The Continuing Puzzle of Committee Outliers.pdf}
}

@article{petitpas,
  title = {The More the Better? Cumulative Issue Ownership and Intra-Campaign Party Switching},
  author = {Petitpas, Adrien and Sciarini, Pascal},
  journaltitle = {Electoral Studies},
  doi = {10/ggj7mk},
  url = {https://www.sciencedirect.com/science/article/pii/S0261379419301167?dgcid=rss_sd_all&utm_source=researcher_app&utm_medium=referral&utm_campaign=RESR_MRKT_Researcher_inbound},
  abstract = {{$<$}p{$>$}We contribute to the literature on short term changes in voters' party preferences (or intra-campaign party switching), by advancing a factor that has been neglected so far: Voters' perceptions about parties' issue competence. We develop a model of party switching that includes both classic predictors and issue ownership considerations. Moreover, in contrast to the usual {$<$}em{$>$}single{$<$}/em{$>$} issue ownership conception focusing on the party deemed most competent to solve the most important problem, we argue that voters base their party choice on their perceptions of parties' competence on a variety of issues, i.e. on {$<$}em{$>$}cumulative{$<$}/em{$>$} issue ownership. We test our model on panel data from the 2015 Swiss election study. The change in competence perceptions appears as a strong predictor of party switching: The higher the increase in the number of issues on which voters see a party as most competent during the campaign, the higher their likelihood to switch to that party.{$<$}/p{$>$}}
}

